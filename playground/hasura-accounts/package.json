{
  "name": "hasura-accounts",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "author": "Gregory Bouteiller <gregory.bouteiller@niama.re>",
  "private": true,
  "scripts": {
    "dev:client": "quasar dev",
    "dev:docker": "docker-compose -p hasura_dev -f config/b.docker.dev.yml up -d",
    "dev:docker:reset" : "docker volume prune -f",
    "dev:hasura": "hasura console",
    "dev:hasura:export": "gq http://192.168.99.101:9101/v1/graphql -H 'X-Hasura-Admin-Secret:niama_api_secret' --introspect > schema.graphql",
    "dev:hasura:init": "hasura migrate apply",
    "dev:server": "nodemon --config config/b.server.nodemon.json",
    "prod:client": "quasar build",
    "prod:serve": "quasar serve dist/spa"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "quasar": "^1.0.0"
  },
  "devDependencies": {
    "@niama/quasar-app-extension-apollo": "^1.0.4",
    "@niama/quasar-app-extension-i18n": "^1.0.2",
    "@niama/quasar-app-extension-pug": "^1.0.1",
    "@niama/quasar-app-extension-starter": "^1.1.2",
    "@niama/quasar-app-extension-tailwindcss": "^1.0.4",
    "@niama/quasar-app-extension-ts": "^1.0.5",
    "@quasar/app": "^1.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
